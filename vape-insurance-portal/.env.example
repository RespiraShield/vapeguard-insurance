# Backend API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id_here

# Dashboard URL
REACT_APP_DASHBOARD_URL=http://localhost:3003

# Environment
REACT_APP_ENV=development

# Feature Flags
# Payment feature flag - set to false to disable payment step temporarily
REACT_APP_PAYMENT_ENABLED=false

# Bill photo upload feature flag - set to true to enable bill photo upload requirement
REACT_APP_BILL_PHOTO_ENABLED=false
